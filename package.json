{
  "name": "@mntm/multipart",
  "version": "1.0.0",
  "author": {
    "name": "Anton Petrov",
    "email": "im@petrov.engineer",
    "url": "https://petrov.engineer"
  },
  "description": "Probably the fastest most compatible lexer and parser for multipart/form-data.",
  "license": "MIT",
  "keywords": [
    "lexer",
    "parser",
    "multipart",
    "form-data",
    "formdata"
  ],
  "homepage": "https://github.com/maxi-team/multipart",
  "bugs": "https://github.com/maxi-team/multipart",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxi-team/multipart.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": "./dist/index.mjs",
  "sideEffects": false,
  "scripts": {
    "build:clear": "shx rm -rf dist",
    "build:types": "tsc",
    "build:esm": "esbuild src/index.ts --bundle --external:buffer --platform=node --target=es6 --minify --keep-names --outfile=dist/index.mjs --format=esm",
    "build:cjs": "esbuild src/index.ts --bundle --external:buffer --platform=node --target=es6 --minify --keep-names --outfile=dist/index.cjs --format=cjs",
    "build": "yarn run build:clear && yarn run build:types && yarn run build:esm && yarn run build:cjs"
  },
  "devDependencies": {
    "@mntm/eslint-config": "^0.4.3",
    "@types/node": "^16.7.13",
    "esbuild": "^0.12.25",
    "eslint": "^7.32.0",
    "shx": "^0.3.3",
    "typescript": "^4.4.2"
  }
}
